    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Specialised.Dialogs\Krypton.Toolkit.Suite.Extended.Specialised.Dialogs 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.MessageDialog\Krypton.Toolkit.Suite.Extended.MessageDialog.csproj" />
    
    <!-- Data Visualization -->
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.DataGridView\Krypton.Toolkit.Suite.Extended.DataGridView 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.AdvancedDataGridView\Krypton.Toolkit.Suite.Extended.AdvancedDataGridView 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Outlook.Grid\Krypton.Toolkit.Suite.Extended.Outlook.Grid 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Data.Visualisation\Krypton.Toolkit.Suite.Extended.Data.Visualisation 2022.csproj" />
    
    <!-- Additional Components -->
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Circular.ProgressBar\Krypton.Toolkit.Suite.Extended.Circular.ProgressBar 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Calendar\Krypton.Toolkit.Suite.Extended.Calendar 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.ComboBox\Krypton.Toolkit.Suite.Extended.ComboBox 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Notifications\Krypton.Toolkit.Suite.Extended.Notifications 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Navi.Suite\Krypton.Toolkit.Suite.Extended.Navi.Suite 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Navigator\Krypton.Toolkit.Suite.Extended.Navigator 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Ribbon\Krypton.Toolkit.Suite.Extended.Ribbon 2022.csproj" />
    
    <!-- Drawing & Design -->
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Drawing\Krypton.Toolkit.Suite.Extended.Drawing 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Drawing.Utilities\Krypton.Toolkit.Suite.Extended.Drawing.Utilities 2022.csproj" />
    
    <!-- Utilities -->
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.IO\Krypton.Toolkit.Suite.Extended.IO 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Compression\Krypton.Toolkit.Suite.Extended.Compression 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.CheckSum.Tools\Krypton.Toolkit.Suite.Extended.CheckSum.Tools 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Error.Reporting\Krypton.Toolkit.Suite.Extended.Error.Reporting 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Settings\Krypton.Toolkit.Suite.Extended.Settings 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Resources\Krypton.Toolkit.Suite.Extended.Resources 2022.csproj" />
    
    <!-- Advanced Features -->
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Dock.Extender\Krypton.Toolkit.Suite.Extended.Dock.Extender 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Floating.Toolbars\Krypton.Toolkit.Suite.Extended.Floating.Toolbars 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Theme.Switcher\Krypton.Toolkit.Suite.Extended.Theme.Switcher 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Forms\Krypton.Toolkit.Suite.Extended.Forms 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.File.Copier\Krypton.Toolkit.Suite.Extended.File.Copier 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Memory.Box\Krypton.Toolkit.Suite.Extended.Memory.Box 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Networking\Krypton.Toolkit.Suite.Extended.Networking 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Software.Updater.Core\Krypton.Toolkit.Suite.Extended.Software.Updater.Core 2022.csproj" />
    <LibraryProject Include="$(SourceDir)\Krypton.Toolkit.Suite.Extended.Language.Model\Krypton.Toolkit.Suite.Extended.Language.Model 2022.csproj" />
    
    <!-- Ultimate Packages (All-in-One) -->
    <UltimatePackage Include="$(UltimateProject)" />
    <UltimatePackage Include="$(UltimateLiteProject)" />
  </ItemGroup>

  <!-- All Configurations for BuildAll target -->
  <ItemGroup>
    <BuildConfiguration Include="Release" />
    <BuildConfiguration Include="Canary" />
    <BuildConfiguration Include="Nightly" />
    <BuildConfiguration Include="Debug" />
  </ItemGroup>

  <!-- ============================================
       Target: ShowConfiguration
       Displays current build configuration
       ============================================ -->
  <Target Name="ShowConfiguration">
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
    <Message Text="  Krypton Extended Toolkit - Build Configuration" Importance="high" />
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
    <Message Text="Configuration : $(Configuration)" Importance="high" />
    <Message Text="Platform      : $(Platform)" Importance="high" />
    <Message Text="Target Branch : $(TargetBranch)" Importance="high" />
    <Message Text="Package Suffix: $(PackageSuffix)" Importance="high" />
    <Message Text="Output Dir    : $(OutputDir)" Importance="high" />
    <Message Text="NuGet Dir     : $(NuGetPackagesDir)" Importance="high" />
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
  </Target>

  <!-- ============================================
       Target: Restore
       Restores NuGet packages for all solutions
       ============================================ -->
  <Target Name="Restore">
    <Message Text="ðŸ”„ Restoring NuGet packages..." Importance="high" />
    
    <Exec Command="dotnet restore &quot;$(MainSolution)&quot;" 
          WorkingDirectory="$(RootDir)" />
    
    <Message Text="âœ… NuGet packages restored successfully" Importance="high" />
  </Target>

  <!-- ============================================
       Target: Clean
       Cleans build output
       ============================================ -->
  <Target Name="Clean">
    <Message Text="ðŸ§¹ Cleaning $(Configuration) configuration..." Importance="high" />
    
    <MSBuild Projects="$(MainSolution)"
             Properties="Configuration=$(Configuration);Platform=$(Platform)"
             Targets="Clean"
             BuildInParallel="$(BuildInParallel)"
             ContinueOnError="$(ContinueOnError)" />
    
    <Message Text="âœ… Clean completed" Importance="high" />
  </Target>

  <!-- ============================================
       Target: Build
       Builds the main solution
       ============================================ -->
  <Target Name="Build" DependsOnTargets="ShowConfiguration;Restore">
    <Message Text="ðŸ”¨ Building $(Configuration) configuration..." Importance="high" />
    
    <MSBuild Projects="$(MainSolution)"
             Properties="Configuration=$(Configuration);Platform=$(Platform)"
             Targets="Build"
             BuildInParallel="$(BuildInParallel)"
             ContinueOnError="$(ContinueOnError)" />
    
    <Message Text="âœ… Build completed successfully" Importance="high" />
  </Target>

  <!-- ============================================
       Target: BuildNuGet
       Builds the NuGet solution
       ============================================ -->
  <Target Name="BuildNuGet" DependsOnTargets="Build">
    <Message Text="ðŸ“¦ Building NuGet solution ($(Configuration))..." Importance="high" />
    
    <MSBuild Projects="$(NuGetSolution)"
             Properties="Configuration=$(Configuration);Platform=$(Platform)"
             Targets="Build"
             BuildInParallel="$(BuildInParallel)"
             ContinueOnError="$(ContinueOnError)" />
    
    <Message Text="âœ… NuGet solution built successfully" Importance="high" />
  </Target>

  <!-- ============================================
       Target: BuildDev
       Builds the development solution
       ============================================ -->
  <Target Name="BuildDev" DependsOnTargets="Build">
    <Message Text="ðŸ”§ Building Dev solution ($(Configuration))..." Importance="high" />
    
    <MSBuild Projects="$(DevSolution)"
             Properties="Configuration=$(Configuration);Platform=$(Platform)"
             Targets="Build"
             BuildInParallel="$(BuildInParallel)"
             ContinueOnError="$(ContinueOnError)" />
    
    <Message Text="âœ… Dev solution built successfully" Importance="high" />
  </Target>

  <!-- ============================================
       Target: BuildUltimate
       Builds the Ultimate all-in-one packages
       ============================================ -->
  <Target Name="BuildUltimate" DependsOnTargets="BuildNuGet">
    <Message Text="â­ Building Ultimate packages ($(Configuration))..." Importance="high" />
    
    <!-- Build Ultimate Package -->
    <MSBuild Projects="$(UltimateProject)"
             Properties="Configuration=$(Configuration);Platform=$(Platform)"
             Targets="Build"
             BuildInParallel="false"
             ContinueOnError="$(ContinueOnError)" />
    
    <!-- Build Ultimate Lite Package -->
    <MSBuild Projects="$(UltimateLiteProject)"
             Properties="Configuration=$(Configuration);Platform=$(Platform)"
             Targets="Build"
             BuildInParallel="false"
             ContinueOnError="$(ContinueOnError)" />
    
    <Message Text="âœ… Ultimate packages built successfully" Importance="high" />
  </Target>

  <!-- ============================================
       Target: Pack
       Creates NuGet packages for all libraries
       ============================================ -->
  <Target Name="Pack" DependsOnTargets="BuildUltimate">
    <Message Text="ðŸ“¦ Creating NuGet packages ($(Configuration))..." Importance="high" />
    <Message Text="Package Suffix: $(PackageSuffix)" Importance="high" />
    
    <!-- Ensure output directory exists -->
    <MakeDir Directories="$(NuGetPackagesDir)" Condition="!Exists('$(NuGetPackagesDir)')" />
    
    <!-- Pack individual library projects -->
    <MSBuild Projects="@(LibraryProject)"
             Properties="Configuration=$(Configuration);PackageOutputPath=$(NuGetPackagesDir);NoBuild=true"
             Targets="Pack"
             BuildInParallel="$(BuildInParallel)"
             ContinueOnError="true" />
    
    <!-- Pack Ultimate packages -->
    <Exec Command="dotnet pack &quot;$(UltimateProject)&quot; --configuration $(Configuration) --output &quot;$(NuGetPackagesDir)&quot; --no-build"
          WorkingDirectory="$(RootDir)"
          ContinueOnError="false" />
    
    <Exec Command="dotnet pack &quot;$(UltimateLiteProject)&quot; --configuration $(Configuration) --output &quot;$(NuGetPackagesDir)&quot; --no-build"
          WorkingDirectory="$(RootDir)"
          ContinueOnError="false" />
    
    <Message Text="âœ… NuGet packages created in: $(NuGetPackagesDir)" Importance="high" />
  </Target>

  <!-- ============================================
       Target: PackUltimateOnly
       Creates only the Ultimate packages
       ============================================ -->
  <Target Name="PackUltimateOnly" DependsOnTargets="BuildUltimate">
    <Message Text="â­ Creating Ultimate packages only ($(Configuration))..." Importance="high" />
    
    <!-- Ensure output directory exists -->
    <MakeDir Directories="$(NuGetPackagesDir)" Condition="!Exists('$(NuGetPackagesDir)')" />
    
    <Exec Command="dotnet pack &quot;$(UltimateProject)&quot; --configuration $(Configuration) --output &quot;$(NuGetPackagesDir)&quot; --no-build"
          WorkingDirectory="$(RootDir)" />
    
    <Exec Command="dotnet pack &quot;$(UltimateLiteProject)&quot; --configuration $(Configuration) --output &quot;$(NuGetPackagesDir)&quot; --no-build"
          WorkingDirectory="$(RootDir)" />
    
    <Message Text="âœ… Ultimate packages created" Importance="high" />
  </Target>

  <!-- ============================================
       Target: Rebuild
       Clean and rebuild
       ============================================ -->
  <Target Name="Rebuild" DependsOnTargets="Clean;Build">
    <Message Text="âœ… Rebuild completed" Importance="high" />
  </Target>

  <!-- ============================================
       Target: BuildAll
       Builds all configurations
       ============================================ -->
  <Target Name="BuildAll">
    <Message Text="ðŸš€ Building all configurations..." Importance="high" />
    
    <MSBuild Projects="$(MSBuildProjectFile)"
             Properties="Configuration=%(BuildConfiguration.Identity);Platform=$(Platform)"
             Targets="Build"
             BuildInParallel="false" />
    
    <Message Text="âœ… All configurations built successfully" Importance="high" />
  </Target>

  <!-- ============================================
       Target: CI
       Complete CI pipeline
       ============================================ -->
  <Target Name="CI" DependsOnTargets="ShowConfiguration;Clean;Build;BuildNuGet;BuildUltimate;Pack">
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
    <Message Text="  âœ… CI Build Completed Successfully!" Importance="high" />
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
    <Message Text="Configuration : $(Configuration)" Importance="high" />
    <Message Text="Target Branch : $(TargetBranch)" Importance="high" />
    <Message Text="Packages Dir  : $(NuGetPackagesDir)" Importance="high" />
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
  </Target>

  <!-- ============================================
       Target: CIAll
       Complete CI pipeline for all configurations
       ============================================ -->
  <Target Name="CIAll">
    <Message Text="ðŸš€ Running CI for all release channels..." Importance="high" />
    
    <!-- Build Release (master) -->
    <MSBuild Projects="$(MSBuildProjectFile)"
             Properties="Configuration=Release;Platform=$(Platform)"
             Targets="CI"
             BuildInParallel="false" />
    
    <!-- Build Canary (canary) -->
    <MSBuild Projects="$(MSBuildProjectFile)"
             Properties="Configuration=Canary;Platform=$(Platform)"
             Targets="CI"
             BuildInParallel="false" />
    
    <!-- Build Nightly (alpha) -->
    <MSBuild Projects="$(MSBuildProjectFile)"
             Properties="Configuration=Nightly;Platform=$(Platform)"
             Targets="CI"
             BuildInParallel="false" />
    
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
    <Message Text="  âœ… All CI Builds Completed Successfully!" Importance="high" />
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
    <Message Text="Release (master) : $(BinDir)\Release" Importance="high" />
    <Message Text="Canary (canary)  : $(BinDir)\Canary" Importance="high" />
    <Message Text="Nightly (alpha)  : $(BinDir)\Nightly" Importance="high" />
    <Message Text="â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•" Importance="high" />
  </Target>

  <!-- ============================================
       Target: ListPackages
       Lists all generated NuGet packages
       ============================================ -->
