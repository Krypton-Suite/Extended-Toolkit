<Project>

	<!--Package data-->
		<PropertyGroup>
			<PackageId Condition="'$(Configuration)' == 'Release'">
				$(PackageId)
			</PackageId>

			<Description Condition="'$(Configuration)' == 'Release'">
				$(Description)
				This package supports all .NET Framework versions starting .NET 4.6.2 and .NET Core 3.1 - .NET 6.
				Also, all libraries are included targeting each specific framework version for performance purposes.
			</Description>
		</PropertyGroup>

</Project>