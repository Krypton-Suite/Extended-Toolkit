<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net48;net5.0-windows;net6.0-windows</TargetFrameworks>
    <TargetFrameworks Condition="'$(TFMs)' == 'all'">net35;net40;net45;net451;net452;net46;net461;net462;net47;net471;net472;net48;netcoreapp3.1;net5.0-windows;net6.0-windows</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <RootNamespace>Zip.Storer</RootNamespace>
    <AssemblyName>Zip.Storer</AssemblyName>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Jaime Olivares</Authors>
  </PropertyGroup>
  
  <PropertyGroup>
    <DocumentationFile>..\..\..\..\Bin\$(configuration)\Third Party\Zip.Storer\Zip.Storer.xml</DocumentationFile>
    <OutputPath>..\..\..\..\Bin\$(configuration)\Third Party\Zip.Storer\</OutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyVersion>3.6.0</AssemblyVersion>
    <FileVersion>3.6.0</FileVersion>
    <Version>3.6.0</Version>
  </PropertyGroup>

</Project>
